citHeader("To cite package 'brlgm' in publications use:")

citEntry(entry="Manual",
    title = {"brglm: Bias reduction in binary-response GLMs"},
    author = personList(as.person("Ioannis Kosmidis")),
    year = {2010},
    note = {"R package version 0.5-5"},
    url = {"http://go.warwick.ac.uk/kosmidis/software"},
    textVersion = paste(
      "Ioannis Kosmidis (2010).",
      "brglm: Bias reduction in binary-response GLMs.",
      "http://go.warwick.ac.uk/kosmidis/software")
)


